{
  "name": "payoneer-calculator",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "lint-staged": {
    "./**/*.{js,mjs,cjs}": [
      "eslint --cache --fix --ext .js,.mjs,.cjs ./",
      "prettier --write \"./**/*.{js,mjs,cjs}\""
    ],
    "./src/**/*.{jsx,tsx}": [
      "eslint --cache --ext .jsx,.tsx ./src/ --fix",
      "prettier --write \"./src/**/*.{jsx,tsx}\""
    ],
    "./index.html": "prettier --write \"./index.html\"",
    "./src/styles/*.css": "prettier --write \"./src/styles/*.css\"",
    "./**/*.json": "prettier --write \"./**/*.json\"",
    "./**/*.{yaml,yml}": "prettier --write \"./**/*.{yaml,yml}\""
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vitejs/plugin-react-swc": "3.0.0",
    "autoprefixer": "10.4.14",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-refresh": "0.3.4",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "postcss": "8.4.24",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "tailwindcss": "3.3.2",
    "typescript": "5.0.2",
    "vite": "4.3.9"
  },
  "engines": {
    "node": "^18",
    "npm": "^9"
  }
}
