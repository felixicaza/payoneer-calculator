{
  "name": "payoneer-calculator",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "astro": "astro",
    "build": "astro check && astro build",
    "dev": "astro dev",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix .",
    "prepare": "simple-git-hooks",
    "preview": "astro preview",
    "start": "astro dev"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "./**/*.{js,mjs,cjs}": [
      "eslint --cache --fix --ext .js,.mjs,.cjs ./",
      "prettier --cache --write \"./**/*.{js,mjs,cjs}\""
    ],
    "./src/**/*.astro": [
      "eslint --cache --fix --ext .astro ./src/",
      "prettier --cache --write \"./src/**/*.astro\""
    ],
    "./src/**/*.tsx": [
      "eslint --cache --fix --ext .tsx ./src/",
      "prettier --cache --write \"./src/**/*.tsx\""
    ],
    "./src/**/*.css": "prettier --cache --write \"./src/**/*.css\"",
    "./**/*.json": "prettier --cache --write \"./**/*.json\"",
    "./**/*.yml": "prettier --cache --write \"./**/*.yml\""
  },
  "dependencies": {
    "@astrojs/check": "0.9.1",
    "@astrojs/react": "3.6.1",
    "@astrojs/tailwind": "5.1.0",
    "@vite-pwa/astro": "0.3.1",
    "astro": "4.13.1",
    "astro-iconify": "1.2.0",
    "firebase": "10.12.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwindcss": "3.4.7",
    "typescript": "5.5.4",
    "vite-plugin-pwa": "0.20.1",
    "workbox-window": "7.1.0"
  },
  "devDependencies": {
    "@eslint-react/eslint-plugin": "1.48.4",
    "@eslint/js": "9.25.0",
    "@types/eslint-plugin-tailwindcss": "3.17.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@typescript-eslint/parser": "8.30.1",
    "eslint": "9.25.0",
    "eslint-plugin-astro": "1.3.1",
    "eslint-plugin-jsonc": "2.20.0",
    "eslint-plugin-jsx-a11y": "6.10.2",
    "eslint-plugin-package-json": "0.29.1",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.19",
    "eslint-plugin-tailwindcss": "3.18.0",
    "eslint-plugin-yml": "1.17.0",
    "globals": "16.0.0",
    "jiti": "2.4.2",
    "lint-staged": "15.2.8",
    "neostandard": "0.12.1",
    "prettier": "3.3.3",
    "prettier-plugin-astro": "0.14.1",
    "prettier-plugin-tailwindcss": "0.6.5",
    "simple-git-hooks": "2.12.1"
  }
}
